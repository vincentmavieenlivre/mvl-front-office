{
  "name": "front-office",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development refine dev --mode development",
    "dev:emulator": "firebase emulators:start --import emulator_dumps/init",
    "build:prod": "tsc && NODE_ENV=production dotenv -e .env.production vite build --mode production",
    "build:staging": " tsc && NODE_ENV=staging dotenv -e .env.staging vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "refine": "refine"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.10",
    "@reduxjs/toolkit": "^2.2.1",
    "@refinedev/cli": "^2.16.26",
    "@refinedev/devtools": "^1.1.34",
    "@refinedev/kbar": "^1.3.6",
    "@refinedev/react-router-v6": "^4.5.5",
    "@refinedev/simple-rest": "^5.0.2",
    "firebase": "^10.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.0",
    "refine-firebase": "^1.2.1"
  },
  "devDependencies": {
    "@refinedev/core": "^4.47.1",
    "@refinedev/mui": "^5.14.4",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "daisyui": "^4.7.2",
    "dotenv": "^16.4.4",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.0"
  },
  "refine": {
    "projectId": "zB8t94-VPwJA0-BmEInX"
  }
}
